{
  "manifest_version": 2,

  "name": "Hexery",
  "description": "Extension for hex.pm packages search",
  "version": "1.0.0.1",
  "version_name": "1.0.1 stable",
  "icons": {
    "48": "icon.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://hex.pm"
  ]
}
